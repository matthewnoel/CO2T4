{
	"name": "co2t4",
	"version": "1.0.0",
	"type": "module",
	"description": "Carbon Dioxide Tolerance Testing & Training Tool",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"prepare": "svelte-kit sync",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:e2e": "playwright test",
		"test": "npm run test:e2e",
		"licenses": "npx generate-license-file --input package.json --output third-party-licenses.txt --overwrite"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/matthewnoel/CO2T4.git"
	},
	"keywords": [
		"svelte"
	],
	"author": "Matthew Noel",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/matthewnoel/CO2T4/issues"
	},
	"homepage": "https://github.com/matthewnoel/CO2T4#readme",
	"devDependencies": {
		"@playwright/test": "^1.57.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.50.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/postcss": "^4.1.18",
		"@tailwindcss/vite": "^4.1.18",
		"autoprefixer": "^10.4.23",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.14.0",
		"generate-license-file": "^4.1.1",
		"globals": "^17.0.0",
		"prettier": "^3.8.0",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"svelte": "^5.46.4",
		"svelte-check": "^4.3.5",
		"tailwindcss": "^4.0.5",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.53.0",
		"vite": "^7.3.1"
	}
}
