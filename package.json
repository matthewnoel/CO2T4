{
	"name": "co2t4",
	"version": "1.0.0",
	"type": "module",
	"description": "Carbon Dioxide Tolerance Testing & Training Tool",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:e2e": "playwright test",
		"test": "npm run test:e2e",
		"licenses": "npx generate-license-file --input package.json --output third-party-licenses.txt --overwrite"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/matthewnoel/CO2T4.git"
	},
	"keywords": [
		"svelte"
	],
	"author": "Matthew Noel",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/matthewnoel/CO2T4/issues"
	},
	"homepage": "https://github.com/matthewnoel/CO2T4#readme",
	"devDependencies": {
		"@playwright/test": "^1.45.3",
		"@sveltejs/adapter-static": "^3.0.6",
		"@sveltejs/kit": "^2.9.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.1",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.16.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
		"generate-license-file": "^3.6.0",
		"globals": "^15.13.0",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^5.3.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.9",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^6.0.1"
	}
}
