{
	"name": "co2t4",
	"version": "1.0.0",
	"type": "module",
	"description": "Carbon Dioxide Tolerance Testing & Training Tool",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:e2e": "playwright test",
		"test": "npm run test:e2e",
		"licenses": "npx generate-license-file --input package.json --output third-party-licenses.txt --overwrite"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/matthewnoel/CO2T4.git"
	},
	"keywords": [
		"svelte"
	],
	"author": "Matthew Noel",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/matthewnoel/CO2T4/issues"
	},
	"homepage": "https://github.com/matthewnoel/CO2T4#readme",
	"devDependencies": {
		"@playwright/test": "^1.50.1",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.17.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/vite": "^4.0.5",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.20.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"generate-license-file": "^3.7.0",
		"globals": "^15.14.0",
		"prettier": "^3.5.0",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^5.19.9",
		"svelte-check": "^4.1.4",
		"tailwindcss": "^4.0.5",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.23.0",
		"vite": "^6.1.0"
	}
}
